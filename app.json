{
  "expo": {
    "name": "DeliveryGo",
    "slug": "DeliveryGo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/app-icon1.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.ReactNativeApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
  [
    "expo-splash-screen",
    {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "cover",          
      "backgroundColor": "#ffffff"
    }
  ]
],

    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "71bdeabf-976a-44e9-99c2-a4d1ee9f2a46"
      }
    }
  }
}
